uages: {\n      type: String,\n      required: false,\n    },\n    borders: {\n      type: String,\n      required: false,\n    },\n  },\n  components: {},\n  data() {\n    return {};\n  },\n  computed: {\n    /*  countryName() {\n      return nation.name.common;\n    },\n    countryCapital() {\n      return nation.capital[0];\n    },\n    countryFlag() {\n      return nation.flags.png;\n    },\n    countryNativeName() {\n      //to extract the last entry in native names\n\n      const lastEntryNativeName = Object.values(nation.name.nativeName).pop()\n        .common;\n\n      return lastEntryNativeName;\n    },\n    countryTopLevelDomain() {\n      return nation.tld[0];\n    },\n    countryCurrencies() {\n      const currenciesList = [];\n\n      for (const currenciesName of Object.values(nation.currencies)) {\n        currenciesList.push(currenciesName.name);\n      }\n      return currenciesList.join(\", \");\n    },\n    countryLanguages() {\n      const countryLanguages = [];\n\n      for (const language of Object.values(nation.languages)) {\n        countryLanguages.push(language);\n      }\n      return countryLanguages.join(\", \");\n    },\n    countryBorders() {\n      return nation.cca3;\n    }, */\n  },\n  created() {\n    //this.loadNation();\n    this.loadNations();\n\n    /*  fetch(`https://restcountries.com/v3.1/name/${this.name}`)\n      .then((res) => res.json())\n      .then((data) => (this.nationDetails = data[0]))\n      .catch((err) => console.error(err)); */\n    //this.loadNation();\n  },\n  methods: {\n    /* loadNation() {\n      fetch(`https://restcountries.com/v3.1/name/${this.name}`)\n        .then((res) => res.json())\n        .then((data) => (this.nationDetails = data[0]))\n        .catch((err) => console.error(err));\n    }, */\n    loadNations() {\n      this.$store.dispatch(\"loadNations\");\n    },\n  },\n};\n</script>\n<style>\n</style>"]}]}