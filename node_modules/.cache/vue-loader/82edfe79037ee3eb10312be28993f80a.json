   },\n    isDark() {\n      return this.$store.getters.isDarkSwitcher;\n    },\n  },\n};\n</script>\n<style scoped>\nli {\n  width: 19rem;\n  height: 400px;\n  border-radius: 10px;\n  box-shadow: 1px 1px 5px gainsboro;\n}\nli img {\n  width: 100%;\n  height: 52%;\n  border-top-left-radius: 10px;\n  border-top-right-radius: 10px;\n  z-index: 1;\n}\n.country-details {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n  padding: 2rem 1.5rem 3rem 1.5rem;\n  height: 48%;\n  border-bottom-right-radius: 10px;\n  border-bottom-left-radius: 10px;\n  top: -0.3rem;\n  z-index: 0;\n}\n.country-details h3 {\n  margin-bottom: 1rem;\n  cursor: pointer;\n}\n@media screen and (min-width: 1200px) {\n  li {\n    width: 22rem;\n    height: 420px;\n  }\n  li img {\n    width: 100%;\n    height: 220px;\n  }\n}\n</style>\n"]}]}