uted: {\n    countryName() {\n      return this.nationDetails.name.common;\n    },\n    countryCapital() {\n      return this.nationDetails.capital[0];\n    },\n    countryFlag() {\n      return this.nationDetails.flags.png;\n    },\n  },\n  created() {\n    this.loadNation();\n    /*  fetch(`https://restcountries.com/v3.1/name/${this.name}`)\n      .then((res) => res.json())\n      .then((data) => (this.nationDetails = data[0]))\n      .catch((err) => console.error(err)); */\n    //this.loadNation();\n  },\n  methods: {\n    loadNation() {\n      fetch(`https://restcountries.com/v3.1/name/${this.name}`)\n        .then((res) => res.json())\n        .then((data) => (this.nationDetails = data[0]))\n        .catch((err) => console.error(err));\n    },\n  },\n};\n</script>\nstyle"]}]}